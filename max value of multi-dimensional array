function largestOfFour(arr) {
  var maxArray = [];
  for(var k = 0; k < arr.length;k++)
  {
    for(i = 0;i < arr[k].length;i++)
    {
      var hiVal = Math.max.apply(null, arr[i]);
      console.log(hiVal);
      //return hiVal;
      maxArray.push(hiVal);
    }
    return maxArray;
  }
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
